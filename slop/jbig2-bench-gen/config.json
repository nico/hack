    "pdfjs_render": "pdfjs_render.mjs",
    "python3": "/usr/bin/python3",
    "jbig2_ref_wrapper": "jbig2_ref_wrapper.py",
    "jbig2_ref": "/Users/thakis/Downloads/T-REC-T.88-201808-I!!SOFT-ZST-E/Software/JBIG2_SampleSoftware-A20180829/source/jbig2",
